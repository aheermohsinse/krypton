                <div class="column is-4">
                  <!-- <div class="document-block has-text-centered">
                    <img src="/img/icons/livechat.svg" />
                    <a href="/#start" class="button k-button k-secondary raised has-gradient is-bold">
                      <span class="text">Live Chat</span>
                      <span class="front-gradient"></span>
                    </a>
                  </div>
                   -->
                </div>
              </div>
            </div>
          </div>
        </div>

        <!-- Spaced divider -->
        <div class="divider is-centered is-spacer"></div>
        <!-- Title & subtitle -->
        <h2 class="title is-light is-semibold has-text-centered is-spaced">System Repartition</h2>

        <!-- Content wrapper -->
        <div class="content-wrapper">
          <div class="columns is-ico-details">
            <div class="column is-4">
              <div class="details-title">Structure</div>
              <ul>
                <li>Maximum Trading Systems amount: 1499</li>
                <li>Trading Systems for sale: 999</li>
                <li>Bot: Algo X</li>
