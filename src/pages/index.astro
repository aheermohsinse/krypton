      <!-- /Container -->
    </section>
    <!-- /Icon Features section -->

    <!-- Side Features section -->
    <section id="big-gradient" class="section is-transparent">
      <!-- Container -->
      <div class="container">
        <!-- Divider -->
        <div class="divider is-centered"></div>
        <!-- Title & subtitle -->
        <h2 class="title is-light is-semibold has-text-centered is-spaced">Intelligent Trading Technology</h2>
        <h4 class="subtitle is-6 is-light has-text-centered is-compact">
          The Algo X Trading Systems is designed to analyze real-time market conditions, adapt to fluctuations, and make dynamic trading decisions. With advanced risk and position management, it carefully determines when to trade and when to stay out, ensuring optimal performance.
        </h4>

        <!-- Content wrapper -->
        <div class="content-wrapper is-large">
          <div class="columns is-vcentered">
            <!-- Feature content -->
            <div class="column is-5 is-offset-1">
              <div class="side-feature-content">
                <h3 class="title is-4 is-light">
                  <img src="/img/icons/ico/bitbanner.svg" alt="" />
                  <span>Trend Identifying</span>
                </h3>
                <div class="divider is-long"></div>
                <p class="is-light">
                  Algo X Trading Systems intelligently analyzes market trends in real-time, detecting strong momentum and potential reversals. It ensures you enter trades at the right time, maximizing profit opportunities while minimizing risk.
                </p>

