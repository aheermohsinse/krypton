                    <img class="animated preFadeInUp fadeInUp" src="/img/illustrations/bounty.svg" alt="" />
                  </div>
                </div>
              </div>
              <!-- /Tab 4 -->


              <!-- Tab 1 -->
              <div class="tab-content" :class=`{
                'is-active': activeTab === 'tab-1',
                '': activeTab != 'tab-1'
            }`>
              <div class="columns is-vcentered tab-content-wrapper">
                



