            </div>

            <!-- Feature image -->
            <div class="column is-7">
              <img class="side-feature" src="/img/illustrations/crypto-mining.svg" alt="" />
            </div>
          </div>
        </div>
        <!-- /Content wrapper -->
      </div>
      <!-- /Container -->
    </section>
    <!-- /Side Features section -->

    <!-- ICO section -->
    <section class="section is-medium is-end">
